   <p xml:id="205" begin="00:05:15.082" end="00:05:17.284">&gt;&gt; APPRECIATE IT AS ALWAYS.</p>
   <p xml:id="206" begin="00:05:16.217" end="00:05:17.818">DIRECTLY AHEAD, HILLARY</p>
   <p xml:id="207" begin="00:05:17.351" end="00:05:19.053">CLINTON CRITICIZING</p>
   <p xml:id="208" begin="00:05:17.885" end="00:05:19.387">PRESIDENT OBAMAS FOREIGN</p>
