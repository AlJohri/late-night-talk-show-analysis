   <p xml:id="153" begin="00:03:19.256" end="00:03:23.260">ALAN COLMES AND CONSERVATIVE</p>
   <p xml:id="154" begin="00:03:21.758" end="00:03:23.627">RADIO TALK SHOW HOST MIKE</p>
   <p xml:id="155" begin="00:03:23.326" end="00:03:24.194">GALLAGHER.</p>
