   <p xml:id="38" begin="00:00:57.606" end="00:01:01.243">&gt;&gt; JOINING US NOW FROM</p>
   <p xml:id="39" begin="00:00:59.374" end="00:01:01.977">WASHINGTON, JOHN FLANNERY A</p>
   <p xml:id="40" begin="00:01:01.376" end="00:01:04.412">FORM ARE FEDERAL PROSECUTOR</p>
