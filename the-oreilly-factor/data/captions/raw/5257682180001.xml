   <p xml:id="10" begin="00:00:16.464" end="00:00:20.168">CLINTON'S PRIVATE EMAIL</p>
   <p xml:id="11" begin="00:00:17.932" end="00:00:21.169">SERVER MADE IT TO ANTHONY</p>
   <p xml:id="12" begin="00:00:20.301" end="00:00:22.504">WEINER'S COMPUTER.</p>
