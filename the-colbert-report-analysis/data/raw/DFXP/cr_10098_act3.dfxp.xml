<?xml version="1.0" encoding="ISO-8859-1"?>
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml" xmlns:tts="http://www.w3.org/ns/ttml#styling" >
<head>
<styling>
<style xml:id='defaultSpeaker' tts:fontFamily='monospace' tts:fontWeight='bold'  tts:backgroundColor='transparent' tts:color='#ffffff' />
<style xml:id='block' tts:backgroundColor='#00000088' />
<style xml:id='rollup' />
</styling>
